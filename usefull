python .\predict.py --task_name hang_bracelets --ckpt_dir ckpt --policy_class ACT --chunk_size 20 --hidden_dim 512 --batch_size 8 --dim_feedforward 2800 --lr 1e-5 --seed 0 --num_epochs 1000